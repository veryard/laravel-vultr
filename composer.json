{
    "name": "veryard/laravel-vultr",
    "description": "Laravel Vultr",
    "keywords": ["laravel", "framework", "vultr"],
    "license": "MIT",
    "authors": [
        {
            "name": "Brad Veryard",
            "email": "bradveryard@gmail.com",
            "homepage": "https://veryard.dev"
        }
    ],
    "require": {
        "php": "^7.2.5 || ^8.0",
        "illuminate/contracts": "^6.0 || ^7.0 || ^8.0 || ^9.0",
        "illuminate/support": "^6.0 || ^7.0 || ^8.0 || ^9.0",
        "veryard/vultr-php-api": "dev-main"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.5.8 || ^9.3.7"
    },
    "autoload": {
        "psr-4": {
            "Vultr\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vultr\\Laravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vultr\\Laravel\\VultrServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
